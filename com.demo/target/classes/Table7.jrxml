<?xml version="1.0" encoding="UTF-8"?>
<jasperReport 
    xmlns="http://jasperreports.sourceforge.net/jasperreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports 
    http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
    name="BackgroundReport"
    pageWidth="595"
    pageHeight="842"
    columnWidth="555"
    leftMargin="20"
    rightMargin="20"
    topMargin="20"
    bottomMargin="20">

    <!-- Background Band -->
    <background>
        <band height="802">
            <image scaleImage="FillFrame">
                <reportElement x="0" y="0" width="555" height="802"/>
                <imageExpression><![CDATA["C:/Users/shind/OneDrive/Pictures/lenovo.jpg"]]></imageExpression>
            </image>
        </band>
    </background>

    <!-- Title -->
    <title>
        <band height="100">
            <staticText>
                <reportElement x="0" y="30" width="555" height="40"/>
                <textElement textAlignment="Center">
                    <font size="22" isBold="true"/>
                </textElement>
                <text><![CDATA[Student Report With Background]]></text>
            </staticText>
        </band>
    </title>

</jasperReport>
